<Alloy>

  <Collection src="hackathons" id="hackathonsId" instance="true" />

  <Window id="hackathonsWindow">
    <TableView id="hackathonsTable">
      <TableViewRow class="row" hackathonId="{id}">
        <Label class="rowName" text="{name}"/>
        <Label class="rowState" text="{state}" />
      </TableViewRow>
    </TableView>
  </Window>

</Alloy>
